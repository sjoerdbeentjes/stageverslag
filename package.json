    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build",
    "data-dump": "dato dump",
    "build-static-prod": "NODE_ENV='production' NODE_VERSION='9.4.0' npm run data-dump && npm run build && npm run generate",
